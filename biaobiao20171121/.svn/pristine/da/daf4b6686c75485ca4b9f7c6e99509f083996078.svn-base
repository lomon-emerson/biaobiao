.img_list li { float: left; width: 13px; height: 13px; margin-right: 5px; background: #feffff; line-height: 13px; text-align: center; color: #ff6503; cursor: pointer; text-indent: 1px; }
.img_list li.on { background: #ff6600; color: white; }
.img_list li:hover { background: #ff6600; color: white; }
