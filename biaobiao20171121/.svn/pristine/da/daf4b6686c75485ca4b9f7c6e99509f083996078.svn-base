/* tch start */
.login-nr { width: 246px; margin-left: auto; margin-right: auto }
.login-button2 { width: 55px; height: 40px; border: none; background: url(../image/login-button.gif); cursor: pointer; }
.ca_radio { padding-left: 10px; padding-bottom: 3px }
.ca_radio label { margin-right: 20px; vertical-align: middle; display: inline-block; cursor: pointer }
.ca_radio input { height: 20px; line-height: 20px; vertical-align: middle; margin-right: 6px }
.txt_t { width: 124px; height: 20px; border: 1px solid #ccc; margin-left: 10px }
.ca_box { overflow: hidden; margin-top: 15px; }
.ca_box dl { float: left }
.ca_box .login-button2 { float: right }
.ca_box dt { font-size: 14px }
.ca_box dd { padding-left: 60px; line-height: 22px }
.ca_box dd a:link { color: #008dcf }
.ca_box p { height: 22px; line-height: 22px }
.ca_alink { padding-top: 3px }
.ca_alink a { margin-left: 20px }
.ca_alink .red_t { margin-right: 20px; margin-left: 10px }
.ca_alink a:link { color: #3c3c3c }
.ca_alink .red_t:link { color: #cb0e12 }
.login-tabtitle ul { cursor: pointer }
.login-nr blockquote { display: none; margin: 0; }
.login-nr blockquote.show { display: block }
/* tch end */
/* lds start */
.fwdh-box { overflow: visible; }
.fwdh-links { height: 200px; position: relative; overflow: hidden; }
.fwdh-links ul { height: 140px; position: absolute; left: 0; top: 0; }
.fwdh-links li { width: 274px; float: left; }
/* lds end */

